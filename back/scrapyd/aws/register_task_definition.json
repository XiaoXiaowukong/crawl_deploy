{
  "family": "scrapyd",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::285445125734:role/ecs_all",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "essential": true,
      "image": "285445125734.dkr.ecr.ap-northeast-3.amazonaws.com/scrapyd:latest",
      "name": "scrapyd-api",
      "portMappings": [
        {
          "containerPort": 6800,
          "hostPort": 6800
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "awslogs-scrapyd",
          "awslogs-region": "ap-northeast-3",
          "awslogs-stream-prefix": "awslogs-example"
        }
      },
      "environment": [
        {
          "name": "MYSQL_HOST",
          "value": "54.250.151.61"
        },
        {
          "name": "MYSQL_PORT",
          "value": "33066"
        },
        {
          "name": "MYSQL_USER",
          "value": "crawl"
        },
        {
          "name": "MYSQL_POSSWORD",
          "value": "methyl@2022"
        },
        {
          "name": "MYSQL_DB",
          "value": "crawl_auction"
        },
        {
          "name": "CALLBACK_API_HOST",
          "value": "54.250.151.61"
        },
        {
          "name": "CALLBACK_API_PROT",
          "value": "8891"
        },
        {
          "name": "AWS_BUCKET",
          "value": "XXX"
        }
      ]
    }
  ]
}
